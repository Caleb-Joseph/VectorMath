#include <iostream>
#include <vector>
int main () 
{
std::vector<int> even ={};
std::vector<int> odd = {};
int total = 0.0;
int oddtotal =1;
int i;
std::vector<int> math={2,4,3,6,1,9};
for(i=0;i<math.size();i++)
{
if ( math[i]% 2 == 0) 
  {
 even.push_back(math[i]);
total = even[i]+ total;
  }
  else{}
oddtotal = math[i] * oddtotal;
  }

std::cout<< "The sum of even numbers is "<< total<<"\n";
std::cout<< "The product of odd numbers is "<< oddtotal<<"\n";
}

